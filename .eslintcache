ndleChange}\r\n                    className=\"ml-auto Round25 form-control\"\r\n                    placeholder=\"Price\"\r\n                  />\r\n                  <label htmlFor=\"Price\">Price</label>\r\n                </div>\r\n                <div className=\"col-12 col-sm-6 mx-auto px-0\">\r\n                  <ul className=\"col-12 mx-auto combineButton RadioToButton d-inline-flex px-0 mb-4 text-center\">\r\n                    <li className=\"w-50\">\r\n                      <input\r\n                        type=\"radio\"\r\n                        id=\"Featured\"\r\n                        name=\"Featured\"\r\n                        defaultChecked\r\n                        value=\"Yes\"\r\n                        onClick={this.handleChange}\r\n                      />\r\n                      <label htmlFor=\"Featured\" className=\"py-2 mb-0 Bold\">\r\n                        Featured\r\n                      </label>\r\n                    </li>\r\n                    <li className=\"w-50\">\r\n                      <input\r\n                        type=\"radio\"\r\n                        id=\"Non-Featured\"\r\n                        name=\"Featured\"\r\n                        value=\"No\"\r\n                        onClick={this.handleChange}\r\n                      />\r\n                      <label htmlFor=\"Non-Featured\" className=\"py-2 mb-0 Bold\">\r\n                        Non-Featured\r\n                      </label>\r\n                    </li>\r\n                  </ul>\r\n                </div>\r\n                <div className=\"col-12  mx-auto px-0\">\r\n                  <textarea\r\n                    id=\"Description\"\r\n                    name=\"Message\"\r\n                    placeholder=\"Your Message\"\r\n                    value={this.state.Message}\r\n                    onChange={this.handleChange}\r\n                    rows=\"5\"\r\n                    className=\"pl-3\"\r\n                    required\r\n      